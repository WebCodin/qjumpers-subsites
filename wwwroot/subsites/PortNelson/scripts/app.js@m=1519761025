(function($)
{$.fn.features=function(oOptions)
{oOptions=$.extend({iAnimTime:700,iDefaultTime:5,iDefaultStartTime:5},oOptions||{});return this.each(function()
{var $oFeatures=$(this);var tNextTimer,tStartTimer;var iActivePanelNum=1;var bAnimationRunning=false;var oNav=$('<ul></ul>').addClass('nav');var oPanels=$oFeatures.find('.feature');var iNumPanels=oPanels.length;var oPanelContainer=$oFeatures.find('.featuresContainer');var iPanelWidth=oPanels.first().width();showNext=function()
{iActivePanelNum++;if(iActivePanelNum>iNumPanels)
{iActivePanelNum=1;}
showPanel(iActivePanelNum,1);}
showPanel=function(iPanelNum,bStartTimer)
{iPanelNum--;oPanelContainer.animate({left:-iPanelWidth*iPanelNum},oOptions.iAnimTime);oNav.find('li').removeClass('active').eq(iPanelNum).addClass('active');stopTimers();if(bStartTimer)
{tNextTimer=setTimeout('showNext();',oOptions.iDefaultTime*1500);if(iActivePanelNum===iNumPanels)
{stopTimers();}}}
stopTimers=function()
{clearTimeout(tNextTimer);clearTimeout(tStartTimer);}
if(iNumPanels>1)
{oPanels.each(function(iCount)
{iCount++;$('<li></li>').text(iCount).click(function()
{showPanel(iCount);}).appendTo(oNav);});$oFeatures.mouseenter(function()
{stopTimers();}).mouseleave(function()
{tStartTimer=setTimeout('showNext();',oOptions.iDefaultStartTime*1000);}).append(oNav);$(window).load(function()
{showPanel(iActivePanelNum,true);});}
return $oFeatures;});};})(jQuery);;

