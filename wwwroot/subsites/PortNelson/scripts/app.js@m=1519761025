populateElement=function(selector)
{defvalue=$(selector).val();$(selector).each(function()
{if($.trim(this.value)=="")
{this.value=defvalue;}}).focus(function()
{if(this.value==defvalue)
{this.value="";}}).blur(function()
{if($.trim(this.value)=="")
{this.value=defvalue;}});}
